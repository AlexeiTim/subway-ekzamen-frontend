{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "src/**/*.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "index.d.ts",
    "vite.config.ts",
    "src/vite-env.d.ts",
  ],
  "exclude": [
    "src/spec/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}